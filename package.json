{
  "name": "react-twitter-embed",
  "version": "4.0.5",
  "description": "React Twitter Embed Components",
  "author": "Saurabh Nemade",
  "license": "MIT",
  "repository": "saurabhnemade/react-twitter-embed",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "cypress:open": "node_modules/.bin/cypress open-ct open",
    "cypress:ci": "node_modules/.bin/cypress run-ct",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "storybook": "start-storybook -p 9009",
    "prepare": "npm run build",
    "lint": "eslint --ext ts,tsx .",
    "lint:fix": "eslint --fix --ext ts,tsx .",
    "build-storybook": "build-storybook",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@cypress/react": "^5.9.4",
    "@cypress/webpack-dev-server": "^1.5.0",
    "@storybook/addon-actions": "^6.4.16",
    "@storybook/addon-essentials": "^6.4.16",
    "@storybook/addon-links": "^6.4.16",
    "@storybook/react": "^6.4.16",
    "@storybook/theming": "^6.4.16",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "cross-env": "^7.0.2",
    "cypress": "^7.2.0",
    "cypress-wait-until": "^1.7.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^3.2.3",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^4.0.3",
    "typescript": "^4.3.5",
    "webpack-dev-server": "^4.7.3"
  },
  "files": [
    "dist"
  ]
}
